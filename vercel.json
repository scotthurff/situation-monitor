{
	"headers": [
		{
			"source": "/api/proxy",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, s-maxage=60, stale-while-revalidate=300"
				}
			]
		},
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				}
			]
		}
	]
}
